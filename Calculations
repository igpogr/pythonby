class Calculations:
    try:
        a = float (input ()) 
    except ValueError:
        print ('Error')            
    try:
        b = float (input ())
    except ValueError:
        print ('Error')
    def addition (a, b):   
        return a + b
    print (addition (a, b)) 

    def multiplication (a, b):        
        return a * b
    print (multiplication (a, b))

    def subtraction (a, b): 
        return a - b
    print (subtraction (a, b))

    def division (a, b):
        try:
            return a / b
        except ZeroDivisionError:
            print ('Error')
    print (division (a, b)) 

